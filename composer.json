{
    "name": "kabiroman/octawire-auth-service-php-client-bundle",
    "description": "Symfony Bundle for Octawire Auth Service PHP Client",
    "type": "symfony-bundle",
    "keywords": ["symfony", "bundle", "auth", "jwt", "octawire", "security"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0",
        "symfony/http-foundation": "^7.0",
        "kabiroman/octawire-auth-service-php-client": "^0.9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/yaml": "^7.3"
    },
    "suggest": {
        "predis/predis": "Required for Redis-based key caching in AuthClient"
    },
    "autoload": {
        "psr-4": {
            "Kabiroman\\Octawire\\AuthService\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kabiroman\\Octawire\\AuthService\\Bundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
